@{
    ViewBag.Title = "Index";
}

<br />
<h3>File Upload</h3>


<div class="row">
    <div class="col-md-4">

        @if (ViewBag.ErrorMessage != null)
        {
        <div class="alert alert-danger" role="alert">
            @ViewBag.ErrorMessage
        </div>            
        }

        <form method="post" enctype="multipart/form-data">

            <label for="file">Filename:</label>
            <input type="file" name="files" id="file" />

            <input type="submit" />
        </form>
    </div>
</div>

